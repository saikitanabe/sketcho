<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
    xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<ui:style>
		.title {
			color: black;
			font-size: 130%;
			font-weight: bold;
		}
	</ui:style>

	<g:HTMLPanel>
		<g:Label ui:field="title" styleName='{style.title}'></g:Label>
	</g:HTMLPanel>	
</ui:UiBinder>