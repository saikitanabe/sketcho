<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.panel {
			padding: 0;
			border-style: solid;
			border-width: 1px;
			border-color: #fff;
			-moz-border-radius: 3px;
			border-radius: 3px;
			white-space: nowrap;
		}
		
		.panel td {
			
		}
		
		.dropimg {
			
		}
		
		.relationship {
			
		}
		
		.button {
			
		}
		
		.button:HOVER {
			background-color: #666666;
		}
		
	</ui:style>
	
	<g:HTMLPanel ui:field="panel">
		<table class="{style.panel} selectConnection" cellspacing="0"
			cellpadding="0">
			<tr>
				<td ui:field="relationbutton"
					class="selectConnection {style.button}">
					<div ui:field="relationarrow" class="icon-conn-directed"></div>
				</td>
				<td ui:field="drop"
					class="{style.button} dropButton">
					<div class="icon-cust-drop-down"></div>
				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 