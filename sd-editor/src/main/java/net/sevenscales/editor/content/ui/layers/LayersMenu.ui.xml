<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
						 xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	</ui:style>
	
	<g:HTMLPanel>
		<div class="btn-group">
			<a ui:field="moveBack" title="Move element to background" class="btn btn-custom btn-context-menu tooltipper block-link">
				<i class="context-icon-move-back"></i>
			</a>
			<a ui:field="moveFront" title="Move element to top" class="btn btn-custom btn-context-menu tooltipper block-link">
				<i class="context-icon-move-front"></i>
			</a>
			<a ui:field="moveBackward" title="Move element backward" class="btn btn-custom btn-context-menu tooltipper block-link">
				<i class="context-icon-move-backward"></i>
			</a>
			<a ui:field="moveForward" title="Move element forward" class="btn btn-custom btn-context-menu tooltipper block-link">
				<i class="context-icon-move-forward"></i>
			</a>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>

