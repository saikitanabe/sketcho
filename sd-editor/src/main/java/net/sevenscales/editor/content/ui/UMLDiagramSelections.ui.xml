<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:net.sevenscales.editor.api.impl">
	<ui:style>		
				
		.focuspanel {
			
		}
		
		.focuspanel:focus {
			outline: none;
		}
		
	</ui:style>

	<ui:image field="umlclass" src="class-white.png"></ui:image>
	<ui:image field="umlusecase" src="usecase-white.png"></ui:image>
	<ui:image field="umlsequence" src="sequence-white.png"></ui:image>
	<ui:image field="umlactor" src="actor-white.png"></ui:image>
	<ui:image field="umlnote" src="note-white.png"></ui:image>
	<ui:image field="elcomments" src="comments-white.png"></ui:image>
	<ui:image field="umlchoice" src="choice-white.png"></ui:image>
	<ui:image field="umlstart" src="start-white.png"></ui:image>
	<ui:image field="umlend" src="end-white.png"></ui:image>
	<ui:image field="umlactivity" src="activity-white.png"></ui:image>
	<ui:image field="umlhfork" src="hfork.png"></ui:image>
	<ui:image field="umlvfork" src="vfork.png"></ui:image>
	<ui:image field="umltext" src="text-white.png"></ui:image>
	<ui:image field="umlpackage" src="package-white.png"></ui:image>
	<ui:image field="umlstorage" src="db-white.png"></ui:image>
	<ui:image field="centtopic" src="centtopic-white.png"></ui:image>
	<ui:image field="maintopic" src="maintopic-white.png"></ui:image>
	<ui:image field="subtopic" src="subtopic-white.png"></ui:image>

	<!-- <g:VerticalPanel styleName="{style.selectionTable}"> -->
	<g:VerticalPanel>
