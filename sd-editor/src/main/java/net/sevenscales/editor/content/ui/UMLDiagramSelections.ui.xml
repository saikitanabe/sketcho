		<g:HorizontalPanel ui:field="diagramGroups">
			<g:VerticalPanel styleName="uml-selection-table class-diagram">
				<c:FastButton ui:field="note" styleName="{style.focuspanel}">
					<c:child>
						<g:SimplePanel styleName="asbutton">
							<g:Image resource="{umlnote}" styleName="uml-selection"></g:Image>
						</g:SimplePanel>
					</c:child>
				</c:FastButton>
				<c:FastButton ui:field="classf" styleName="{style.focuspanel}">
					<c:child>
						<g:SimplePanel styleName="asbutton">
							<g:Image resource="{umlclass}" styleName="uml-selection"></g:Image>
						</g:SimplePanel>
					</c:child>
				</c:FastButton>
				<c:FastButton ui:field="umlpackagef" styleName="{style.focuspanel}">
					<c:child>
						<g:SimplePanel styleName="asbutton">
							<g:Image resource="{umlpackage}" styleName="uml-selection"></g:Image>
						</g:SimplePanel>
					</c:child>
				</c:FastButton>
			</g:VerticalPanel>
			<g:VerticalPanel styleName="uml-selection-table use-case-diagram">
				<c:FastButton ui:field="actor" styleName="{style.focuspanel}">
					<c:child>
						<g:SimplePanel styleName="asbutton">
							<g:Image resource="{umlactor}" styleName="uml-selection"></g:Image>
						</g:SimplePanel>
					</c:child>
				</c:FastButton>
				<c:FastButton ui:field="usecase" styleName="{style.focuspanel}">
					<c:child>
						<g:SimplePanel styleName="asbutton">
							<g:Image resource="{umlusecase}" styleName="uml-selection"></g:Image>
						</g:SimplePanel>
					</c:child>
				</c:FastButton>
				<c:FastButton ui:field="comments" styleName="{style.focuspanel}">
					<c:child>
						<g:SimplePanel styleName="asbutton">
							<g:Image resource="{elcomments}" styleName="uml-selection"></g:Image>
						</g:SimplePanel>
					</c:child>
				</c:FastButton>
			</g:VerticalPanel>
			<g:VerticalPanel styleName="uml-selection-table activity-diagram">
				<c:FastButton ui:field="start" styleName="{style.focuspanel}">
					<c:child>
						<g:SimplePanel styleName="asbutton">
							<g:Image resource="{umlstart}" styleName="uml-selection"></g:Image>
						</g:SimplePanel>
					</c:child>
				</c:FastButton>
				<c:FastButton ui:field="activity" styleName="{style.focuspanel}">
					<c:child>
						<g:SimplePanel styleName="asbutton">
							<g:Image resource="{umlactivity}" styleName="uml-selection"></g:Image>
						</g:SimplePanel>
					</c:child>
				</c:FastButton>
				<c:FastButton ui:field="choice" styleName="{style.focuspanel}">
					<c:child>
						<g:SimplePanel styleName="asbutton">
							<g:Image resource="{umlchoice}" styleName="uml-selection"></g:Image>
						</g:SimplePanel>
					</c:child>
				</c:FastButton>
				<c:FastButton ui:field="hfork" styleName="{style.focuspanel}">
					<c:child>
						<g:SimplePanel styleName="asbutton uml-hfork">
							<g:Image resource="{umlhfork}"></g:Image>
						</g:SimplePanel>
					</c:child>
				</c:FastButton>
				<c:FastButton ui:field="vfork" styleName="{style.focuspanel}">
					<c:child>
						<g:SimplePanel styleName="asbutton">
							<g:Image resource="{umlvfork}"></g:Image>
						</g:SimplePanel>
					</c:child>
				</c:FastButton>
				<c:FastButton ui:field="end" styleName="{style.focuspanel}">
					<c:child>
						<g:SimplePanel styleName="asbutton">
							<g:Image resource="{umlend}"></g:Image>
						</g:SimplePanel>
					</c:child>
				</c:FastButton>
			</g:VerticalPanel>
			<g:VerticalPanel styleName="uml-selection-table sequence-diagram">
				<c:FastButton ui:field="sequence" styleName="{style.focuspanel}">
					<c:child>
						<g:SimplePanel styleName="asbutton">
							<g:Image resource="{umlsequence}" styleName="uml-selection"></g:Image>
						</g:SimplePanel>
					</c:child>
				</c:FastButton>
				<c:FastButton ui:field="text" styleName="{style.focuspanel}">
					<c:child>
						<g:SimplePanel styleName="asbutton">
							<g:Image resource="{umltext}" styleName="uml-selection"></g:Image>
						</g:SimplePanel>
					</c:child>
				</c:FastButton>
				<c:FastButton ui:field="storage" styleName="{style.focuspanel}">
					<c:child>
						<g:SimplePanel styleName="asbutton">
							<g:Image resource="{umlstorage}" styleName="uml-selection"></g:Image>
						</g:SimplePanel>
					</c:child>
				</c:FastButton>
			</g:VerticalPanel>
			<g:VerticalPanel styleName="uml-selection-table mindmap-diagram">
				<c:FastButton ui:field="_centtopic" styleName="{style.focuspanel}">
					<c:child>
						<g:SimplePanel styleName="asbutton">
							<g:Image resource="{centtopic}" styleName="uml-selection"></g:Image>
						</g:SimplePanel>
					</c:child>
				</c:FastButton>
				<c:FastButton ui:field="_maintopic" styleName="{style.focuspanel}">
					<c:child>
						<g:SimplePanel styleName="asbutton">
							<g:Image resource="{maintopic}" styleName="uml-selection"></g:Image>
						</g:SimplePanel>
					</c:child>
				</c:FastButton>
				<c:FastButton ui:field="_subtopic" styleName="{style.focuspanel}">
					<c:child>
						<g:SimplePanel styleName="asbutton">
							<g:Image resource="{subtopic}" styleName="uml-selection"></g:Image>
						</g:SimplePanel>
					</c:child>
				</c:FastButton>
				<c:FastButton ui:field="_mindnote" styleName="{style.focuspanel}">
					<c:child>
						<g:SimplePanel styleName="asbutton">
							<g:Image resource="{umlnote}" styleName="uml-selection"></g:Image>
						</g:SimplePanel>
					</c:child>
				</c:FastButton>
				<c:FastButton ui:field="_comments" styleName="{style.focuspanel}">
					<c:child>
						<g:SimplePanel styleName="asbutton">
							<g:Image resource="{elcomments}" styleName="uml-selection"></g:Image>
						</g:SimplePanel>
					</c:child>
				</c:FastButton>
			</g:VerticalPanel>
		</g:HorizontalPanel>
	</g:VerticalPanel>
	<!-- </g:VerticalPanel> -->

</ui:UiBinder> 