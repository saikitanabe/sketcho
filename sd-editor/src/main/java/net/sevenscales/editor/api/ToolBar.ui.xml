    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:xlink="http://www.w3.org/1999/xlink">
	<ui:style>
	</ui:style>
	<g:HTMLPanel>
		<ul class="ul-none ul-horizontal">
			<li ui:field="map">
